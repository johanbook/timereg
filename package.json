{
  "name": "timereg",
  "version": "1.0.0",
  "description": "A tool for generating time reports",
  "main": "index.js",
  "bin": "bin/index.js",
  "scripts": {
    "build-linux-x64": "pkg . --targets node16-linux-x64",
    "build-macos-x64": "pkg . --targets node16-macos-x64",
    "build-win-x64": "pkg . --targets node16-win-x64",
    "release": "standard-version",
    "test": "jest",
    "test:coverage": "jest --ci --coverage",
    "transpile": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pkg": {
    "outputPath": "dist"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/luxon": "^3.0.1",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3"
  },
  "dependencies": {
    "@types/jira-client": "^7.1.4",
    "commander": "^9.4.1",
    "jira-client": "^8.2.0",
    "luxon": "^3.0.4",
    "pkg": "^5.8.0",
    "typescript": "^4.8.4"
  }
}
